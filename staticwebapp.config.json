{
    "navigationFallback": {
        "rewrite": "/index.html",
        "exclude": [
            "/images/*.{png,jpg,gif}",
            "/css/*",
            "/js/*",
            "/api/*"
        ]
    },
    "routes": [
        {
            "route": "/*",
            "serve": "/index.html",
            "statusCode": 200
        }
    ],
    "globalHeaders": {
        "cache-control": "no-cache",
        "X-Content-Type-Options": "nosniff",
        "X-Frame-Options": "DENY",
        "X-XSS-Protection": "1; mode=block"
    },
    "mimeTypes": {
        ".json": "text/json",
        ".js": "text/javascript",
        ".css": "text/css",
        ".html": "text/html",
        ".png": "image/png",
        ".jpg": "image/jpeg",
        ".gif": "image/gif",
        ".svg": "image/svg+xml",
        ".ico": "image/x-icon"
    }
}